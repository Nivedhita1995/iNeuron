   "source": [
    "#1. Add the current date to the text file today.txt as a string."
   ]
  },
  {
   "cell_type": "raw",
   "id": "905b555e",
   "metadata": {},
   "source": [
    "import datetime\n",
    "def create_file():\n",
    "    with open (\"today.txt\",\"w\") as file:\n",
    "        file.write(datetime.datetime.now().strftime(\"%d-%m-%Y\"))\n",
    "        file.close()\n",
    "create_file()"
   ]
