   "source": [
    "def empty_list_remove(input_list):\n",
    "    new_list = []\n",
    "    for i in input_list:\n",
    "        if i:\n",
    "            new_list.append(i)\n",
    "    return new_list\n",
    "\n",
    "input_list = [1,2,[],3,4,5,[],6,7,8,9]\n",
    "\n",
    "print(f\"The original list is : {input_list}\")\n",
    "\n",
    "print(f\"List after empty list removal : {empty_list_remove(input_list)}\")\n",
    "\n",
    "               "
