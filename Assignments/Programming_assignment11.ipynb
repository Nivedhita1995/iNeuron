   "source": [
    "#2. Write a Python program for removing i-th character from a string?"
   ]
  },
  {
   "cell_type": "raw",
   "id": "5b8b3dc1",
   "metadata": {},
   "source": [
    "def remove_ith_character(string, i):\n",
    "    initial = string[:i]\n",
    "    final = string[i+1:]\n",
    "    result_string = initial + final\n",
    "    return result_string\n",
    "string = \"Computer Science\"\n",
    "i=3\n",
    "result_string = remove_ith_character(string, i)\n",
    "print(result_string)"
   ]
