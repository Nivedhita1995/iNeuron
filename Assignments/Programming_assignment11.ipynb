    "#6. Write a Python to find all duplicate characters in string?"
   ]
  },
  {
   "cell_type": "raw",
   "id": "70acda71",
   "metadata": {},
   "source": [
    "def duplicate_characters(string):\n",
    "    chars = {}\n",
    "    for i in string:\n",
    "        if i not in chars:\n",
    "            chars[i] = 1\n",
    "        else:\n",
    "            chars[i] +=1\n",
    "            \n",
    "\n",
    "    duplicates = []\n",
    "\n",
    "\n",
    "    for i, count in chars.items():\n",
    "        if count > 1: \n",
    "            duplicates.append(i)\n",
    "\n",
    "    return duplicates\n",
    "\n",
    "\n",
    "print(duplicate_characters(\"Data Science\"))\n"
   ]
