   "source": [
    "#1. Write a Python program to Extract Unique values dictionary values?"
   ]
  },
  {
   "cell_type": "raw",
   "id": "73d0407c",
   "metadata": {},
   "source": [
    "def extract_unique_values(dictionary):\n",
    "    unique_values = set()\n",
    "    for k,v in dictionary.items():\n",
    "        if v not in unique_values:\n",
    "            unique_values.add(v)\n",
    "    \n",
    "    return list(unique_values)\n",
    "\n",
    "dictionary = {'key': 1, 'key1': 2, 'key2': 3, 'key3': 1, 'key4': 2}\n",
    "extract_unique_values(dictionary)\n"
   ]
  },
  {
